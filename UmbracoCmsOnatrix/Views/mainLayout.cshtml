@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    Layout = null;
    var pageTitle = !string.IsNullOrEmpty(ViewBag.Title) ? $"(ViewBag.Title)- Onatrix" : "Onatrix - Finance Solutions For Your Company ";
}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width-device-width, initial-scale=1.0">
        <link rel="stylesheet" href="https://kit.fontawesome.com/a576612222.css" crossorigin="anonymous">
        <link rel="stylesheet" href="/css/style.css"> 
        <link rel="stylesheet" href="/css/main.css">  
        <link rel="stylesheet" href="/css/theme.css">

        <title>@ViewBag.Title</title>
    </head>
    <body>
        <div class="wrapper">
            <header>@await Html.PartialAsync("headerSectionPartial")</header>

             <main>
                 @RenderBody()
             </main>
       
            <footer>@await Html.PartialAsync("blocklist/footerSectionPartial")</footer>

        </div>
        <script src="/script/main.js"></script>   
    </body>
<html>